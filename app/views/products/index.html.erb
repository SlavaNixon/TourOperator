<H2 align="left">Каталог</H2>
<div class="container">
    <div class="row">
      <% @product.each do |prod| %>
        <div class="col-6">
          <%= image_tag prod.product_images.find_by(title: "Title").image, align:"left", class:"mb-3 w-50" %>
          <div>
              <%= link_to prod.name, product_path(prod.id) %>
              <div class="mt-3">Цена поездки: <%= prod.price %> ₽</div>
          </div>
        </div>
      <% end %>
    </div>
</div>
